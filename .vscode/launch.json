{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Emu80qt",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}/dist",
            "program": "${workspaceFolder}/dist/Emu80qt",
            "args": [],
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}